{
  "builds": [{
    "src": "Mining/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  }],
  "routes": [
    {
      "src": "/(servicios.html)",
      "dest": "Mining/wsgi.py"
    }
  ],
  "rewrites": [
    { "source": "/servicios", "destination": "https://prueba2-taupe.vercel.app/Mining_cap/App/templates/App/servicios.html" }
  ]
}

